{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Action": [
                "iam:GetAccountSummary",
                "iam:ListMFADevices",
                "iam:ListAccessKeys",
                "iam:ListPolicies",
                "iam:CreatePolicy",
                "iam:DeletePolicy",
                "iam:GetPolicyVersion",
                "iam:ListEntitiesForPolicy",
                "iam:ListPolicyVersions",
                "iam:CreatePolicyVersion",
                "iam:DeletePolicyVersion",
                "iam:GetPolicy"
            ],
            "Effect": "Allow",
            "Resource": "*"
        },
        {
            "Action": [
                "access-analyzer:ValidatePolicy"
            ],
            "Effect": "Allow",
            "Resource": "*"
        }
    ]
}
